@model List<Post>
{
  for()
}
<table class="table table-striped table-dark">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">First</th>
      <th scope="col">Last</th>
      <th scope="col">Handle</th>
    </tr>
  </thead>
  <tbody>
    @foreach (var item in Model)
    {
      <tr>
      <th scope="row">@item.Id</th>
      <td></td>
      <td></td>
      <td></td>
    </tr>
    }
  </tbody>
</table>